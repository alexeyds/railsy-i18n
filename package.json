{
  "name": "railsy-i18n",
  "version": "0.3.0",
  "description": "Node.js adaptation of Ruby I18n library",
  "main": "dist/railsy-i18n.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "./test.sh test/lib",
    "lint": "eslint {lib,test}/**/*{js,jsx}",
    "test-dist": "./test.sh test/dist",
    "prepare": "npm test && npm run-script build && npm run-script test-dist",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexeyds/railsy-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/alexeyds/railsy-i18n/issues"
  },
  "homepage": "https://github.com/alexeyds/railsy-i18n",
  "keywords": [
    "i18n",
    "localization",
    "internationalization",
    "translation",
    "pluralization",
    "interpolation"
  ],
  "author": "Alexey D",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "enhanced-tape": "^0.1.1",
    "enhanced-tape-runner": "^1.1.2",
    "eslint": "^6.6.0",
    "rollup": "^1.26.0",
    "rollup-plugin-babel": "^4.3.3",
    "tap-spec-dot": "^0.1.2",
    "tape": "^4.11.0"
  }
}
